{
	"private": true,
	"name": "accurtype",
	"license": "MIT",
	"type": "commonjs",
	"author": "e0selmy4v",
	"scripts": {
		"prepare": "husky",
		"commit": "cz",
		"prefmt": "prettier -w .",
		"fmt": "eslint . --fix",
		"test:lint": "eslint test/style.ts --no-eslintrc -c config/eslint.style.js --no-ignore --report-unused-disable-directives",
		"prelint": "prettier -c .",
		"lint": "eslint .",
		"test": "tsc"
	},
	"devDependencies": {
		"@commitlint/cli": "^18.6.1",
		"@stylistic/eslint-plugin": "^1.6.0",
		"@type-challenges/utils": "^0.1.1",
		"@typescript-eslint/eslint-plugin": "^6.20.0",
		"@typescript-eslint/parser": "^6.20.0",
		"commitizen": "^4.3.0",
		"cz-customizable": "^7.0.0",
		"eslint": "^8.56.0",
		"eslint-plugin-expect-type": "^0.3.0",
		"husky": "^9.0.10",
		"prettier": "^3.2.5",
		"typescript": "^4.8.4"
	},
	"prettier": {
		"printWidth": 200,
		"tabWidth": 4,
		"useTabs": true,
		"semi": true,
		"singleQuote": true,
		"jsxSingleQuote": false,
		"trailingComma": "all"
	},
	"config": {
		"cz-customizable": {
			"config": "./config/cz-config.js"
		},
		"commitizen": {
			"path": "node_modules/cz-customizable"
		}
	}
}
